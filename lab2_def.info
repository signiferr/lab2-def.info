import string

COMMON_WORDS = ["the", "and", "to", "of", "you", "that", "have", "for", "with", "not", "this", "but", "can"]

def decode_with_shift(text, shift):
    return "".join(
        chr((ord(c) - (ord('A') if c.isupper() else ord('a')) - shift) % 26 + (ord('A') if c.isupper() else ord('a')))
        if c.isalpha() else c for c in text
    )

def score_english(text):
    return sum(text.lower().count(word) for word in COMMON_WORDS)

def find_best_shift(text):
    return max((decode_with_shift(text, s) for s in range(26)), key=score_english)

print(find_best_shift(input()))
